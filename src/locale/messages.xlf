          <context context-type="linenumber">14</context>
        </context-group>
      </trans-unit>
      <trans-unit id="termsOfUsageCondition2Text" datatype="html">
        <source>
			When you register or submit your personal information in any 5GTANGO Portal/API, we do not share that information with third parties without your
			permission. It is only used for the purposes stated above.
		</source>
        <context-group purpose="location">
          <context context-type="sourcefile">app/authentication/terms-of-usage/terms-of-usage.component.html</context>
          <context context-type="linenumber">15</context>
        </context-group>
      </trans-unit>
      <trans-unit id="termsOfUsageCondition3" datatype="html">
        <source>3. Security of your Personal Information</source>
        <context-group purpose="location">
